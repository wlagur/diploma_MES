<Window x:Class="MES_system_third.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        Title="MES"
        Width="1350"
        Height="600">
    <Grid>

        <Menu Background="White" IsMainMenu="True">
            <MenuItem Header="_Файл">
                <MenuItem Header="_Сохранить" />
                <MenuItem Header="_Выйти" />
            </MenuItem>
            <MenuItem Header="_Помощь">
                <MenuItem Header="_Справка" />
            </MenuItem>
        </Menu>

        <TabControl Margin="0,21,0,-21">
            <TabItem Header="Заказы">
                <Grid>
                    <ListBox Width="150"
                             Height="185"
                             Margin="10,10,0,0"
                             HorizontalAlignment="Left"
                             VerticalAlignment="Top"
                             DisplayMemberPath="Name"
                             FontSize="9"
                             ItemsSource="{Binding Path=ListOfOrders}"
                             SelectedItem="{Binding Path=NewOrder,
                                                    Mode=TwoWay}" />
                    <RadioButton Margin="425,70,702,446"
                                 Content="Новый автомобиль"
                                 GroupName="new_car"
                                 IsChecked="{Binding Path=flag_new_car, Mode=TwoWay}" />
                    <RadioButton Margin="165,70,916,446"
                                 Content="Существующий автомобиль"
                                 GroupName="new_car" />

                    <DatePickerTextBox Margin="270,10,916,506" Text="{Binding Path=NewOrder.dateTime, Mode=TwoWay}"/>
                    <CheckBox Margin="484,16,702,500" IsChecked="{Binding Path=NewOrder.status}" Content="Статус"/>
                    <ComboBox Width="150"
                              Height="25"
                              Margin="270,40,0,0"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Top"
                              DisplayMemberPath="FullName"
                              SelectedItem="{Binding Path=NewOrder.worker, Mode=TwoWay}"
                              ItemsSource="{Binding Path=ListOfWorkers}" />
                    <Label Width="100"
                           Height="25"
                           Margin="165,10,0,0"
                           HorizontalAlignment="Left"
                           VerticalAlignment="Top"
                           Content="Дата и время" />
                    <Label Width="100"
                           Height="25"
                           Margin="165,40,0,0"
                           HorizontalAlignment="Left"
                           VerticalAlignment="Top"
                           Content="Рабочий"
                           RenderTransformOrigin="0.53,0.56" />
                    <Grid IsEnabled="{Binding Path=flag_old_car}">
                        <ComboBox Width="150"
                                  Height="25"
                                  Margin="270,100,0,0"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Top" 
                                  DisplayMemberPath="registrNumber"
                                  SelectedItem="{Binding Path=NewOrder.car, Mode=TwoWay}"
                                  ItemsSource="{Binding Path=ListOfCars}"/>
                        <Label Width="100"
                               Height="25"
                               Margin="165,100,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Content="Автомобиль" />
                    </Grid>
                    <Grid IsEnabled="{Binding Path=flag_new_car}">
                        <TextBox Width="125"
                                 Height="25"
                                 Margin="510,100,0,0"
                                 HorizontalAlignment="Left"
                                 VerticalAlignment="Top"
                                 Text="{Binding Path=NewOrder.car.vincod,
                                                Mode=TwoWay}" />
                        <TextBox Width="125"
                                 Height="25"
                                 Margin="510,130,0,0"
                                 HorizontalAlignment="Left"
                                 VerticalAlignment="Top"
                                 Text="{Binding Path=NewOrder.car.registrNumber,
                                                Mode=TwoWay}" />
                        <ComboBox Width="125"
                                  Height="25"
                                  Margin="510,160,0,0"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Top" 
                                  DisplayMemberPath="nameMarkOfCar"
                                  SelectedItem="{Binding Path=NewOrder.car.modelofcar.markofcar, Mode=TwoWay}"
                                  ItemsSource="{Binding Path=ListOfMarks}"/>
                        <ComboBox Width="125"
                                  Height="25"
                                  Margin="510,190,0,0"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Top" 
                                  DisplayMemberPath="nameModelOfCar"
                                  SelectedItem="{Binding Path=NewOrder.car.modelofcar, Mode=TwoWay}"
                                  ItemsSource="{Binding Path=ListOfModels}"/>
                        <ComboBox Width="125"
                                  Height="25"
                                  Margin="510,220,0,0"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Top" 
                                  DisplayMemberPath="nameColorOfCar"
                                  SelectedItem="{Binding Path=NewOrder.car.colorofcar, Mode=TwoWay}"
                                  ItemsSource="{Binding Path=ListOfColors}"/>
                        
                        <Label Width="80"
                               Height="25"
                               Margin="425,100,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Content="VIN-код" />
                        <Label Width="80"
                               Height="25"
                               Margin="425,130,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Content="Номер" />
                        <Label Width="80"
                               Height="25"
                               Margin="425,160,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Content="Марка" />
                        <Label Width="80"
                               Height="25"
                               Margin="425,190,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Content="Модель" />
                        <Label Width="80"
                               Height="25"
                               Margin="425,220,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Content="Цвет" />

                        <RadioButton Margin="425,262,702,254"
                                 Content="Новый клиент"
                                 GroupName="new_client"
                                 IsChecked="{Binding Path=flag_new_client, Mode=TwoWay}" />
                        <RadioButton Margin="165,262,916,254"
                                 Content="Существующий клиент"
                                 GroupName="new_client" />

                        <Grid IsEnabled="{Binding Path=flag_old_client}">
                            <Label Width="80"
                               Height="25"
                               Margin="161,291,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Content="Клиент" />
                            <ComboBox Width="125"
                                  Height="25"
                                  Margin="246,291,0,0"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Top" 
                                  DisplayMemberPath="FullName"
                                  SelectedItem="{Binding Path=NewOrder.car.client, Mode=TwoWay}"
                                  ItemsSource="{Binding Path=ListOfClients}"/>
                        </Grid>
                        <Grid IsEnabled="{Binding Path=flag_new_client}" Margin="0,195,0,-195">
                            <TextBox Width="125"
                                 Height="25"
                                 Margin="510,100,0,0"
                                 HorizontalAlignment="Left"
                                 VerticalAlignment="Top"
                                 Text="{Binding Path=NewOrder.car.client.lastName,
                                                Mode=TwoWay}" />
                            <TextBox Width="125"
                                 Height="25"
                                 Margin="510,130,0,0"
                                 HorizontalAlignment="Left"
                                 VerticalAlignment="Top"
                                 Text="{Binding Path=NewOrder.car.client.firstName,
                                                Mode=TwoWay}" />
                            <TextBox Width="125"
                                 Height="25"
                                 Margin="510,160,0,0"
                                 HorizontalAlignment="Left"
                                 VerticalAlignment="Top"
                                 Text="{Binding Path=NewOrder.car.client.middleName,
                                                Mode=TwoWay}" />
                            <TextBox Width="125"
                                 Height="25"
                                 Margin="510,190,0,0"
                                 HorizontalAlignment="Left"
                                 VerticalAlignment="Top"
                                 Text="{Binding Path=NewOrder.car.client.phone,
                                                Mode=TwoWay}" />
                            <TextBox Width="125"
                                 Height="25"
                                 Margin="510,220,0,0"
                                 HorizontalAlignment="Left"
                                 VerticalAlignment="Top"
                                 Text="{Binding Path=NewOrder.car.client.email,
                                                Mode=TwoWay}" />
                            <Label Width="80"
                               Height="25"
                               Margin="425,100,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Content="Фамилия" />
                            <Label Width="80"
                               Height="25"
                               Margin="425,130,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Content="Имя" />
                            <Label Width="80"
                               Height="25"
                               Margin="425,160,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Content="Отчество" />
                            <Label Width="80"
                               Height="25"
                               Margin="425,190,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Content="Телефон" />
                            <Label Width="80"
                               Height="25"
                               Margin="425,220,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Content="E-mail" />
                        </Grid>
                    </Grid>
                    <Button Width="150"
                            Height="25"
                            Margin="165,467,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Command="{Binding AddOrderCommand}"
                            Content="Добавить заказ" />
                </Grid>
            </TabItem>
            <TabItem Header="Планирование">
                <Grid>
                    <ListBox Width="150"
                             Height="185"
                             Margin="10,10,0,0"
                             HorizontalAlignment="Left"
                             VerticalAlignment="Top"
                             DisplayMemberPath="Name"
                             FontSize="9"
                             ItemsSource="{Binding Path=ListOfOrders}"
                             SelectedItem="{Binding SelectedOrder,
                                                    Mode=TwoWay}" />

                    <Button Width="150"
                            Height="25"
                            Margin="10,260,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            BorderBrush="{Binding ColorButtonSave}"
                            Command="{Binding SaveCommand}"
                            Content="Сохранить" />
                    <Button Width="150"
                            Height="25"
                            Margin="10,230,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Command="{Binding AddProcessCommand}"
                            Content="Добавить процес">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="Click">
                                <i:InvokeCommandAction Command="{Binding CommandColorButtonSave}" />
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </Button>
                    <Button Width="150"
                            Height="25"
                            Margin="10,200,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Command="{Binding DeleteProcessCommand}"
                            Content="Удалить процес">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="Click">
                                <i:InvokeCommandAction Command="{Binding CommandColorButtonSave}" />
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </Button>

                    <ListView Width="820"
                              Height="275"
                              Margin="165,10,0,0"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Top"
                              ItemsSource="{Binding Path=ListOfProcesses}"
                              SelectedItem="{Binding Path=SelectedProcess,
                                                     Mode=TwoWay}">
                        <ListView.ItemContainerStyle>
                            <Style TargetType="{x:Type ListViewItem}">
                                <Setter Property="Background" Value="{Binding Path=operation.ColorBrush}" />
                            </Style>
                        </ListView.ItemContainerStyle>
                        <ListView.View>
                            <GridView>
                                <GridViewColumn Header="Операция">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal">
                                                <ComboBox Width="170"
                                                          DisplayMemberPath="Name"
                                                          ItemsSource="{Binding RelativeSource={RelativeSource AncestorType=ListBox},
                                                                                Path=DataContext.ListOfOperations}"
                                                          SelectedItem="{Binding Path=operation,
                                                                                 Mode=TwoWay}">
                                                    <ComboBox.Background>
                                                        <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.HotTrackColorKey}}" />
                                                    </ComboBox.Background>
                                                    <i:Interaction.Triggers>
                                                        <i:EventTrigger EventName="SelectionChanged">
                                                            <i:InvokeCommandAction Command="{Binding RelativeSource={RelativeSource AncestorType=ListBox}, Path=DataContext.CommandColorButtonSave}" />
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>
                                                </ComboBox>
                                            </StackPanel>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                                <GridViewColumn Header="Деталь">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal">
                                                <ComboBox Width="150"
                                                          ItemsSource="{Binding Path=ListProcess_has_detail}"
                                                          SelectedItem="{Binding Path=detail,
                                                                                 Mode=TwoWay}">
                                                    <ComboBox.ItemTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Horizontal">
                                                                <CheckBox IsChecked="{Binding FlagProcess_has_detail}" />
                                                                <TextBlock Text="{Binding Path=detail.standartdetail.nameStandartDetail}" />
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </ComboBox.ItemTemplate>
                                                    <i:Interaction.Triggers>
                                                        <i:EventTrigger EventName="SelectionChanged">
                                                            <i:InvokeCommandAction Command="{Binding RelativeSource={RelativeSource AncestorType=ListBox}, Path=DataContext.CommandColorButtonSave}" />
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>
                                                </ComboBox>
                                            </StackPanel>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                                <GridViewColumn Header="Начало">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <DatePickerTextBox Width="130"
                                                               Background="{Binding Path=operation.ColorBrush}"
                                                               Text="{Binding Path=dateTimeStart,
                                                                              Mode=TwoWay}">
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="SelectionChanged">
                                                        <i:InvokeCommandAction Command="{Binding RelativeSource={RelativeSource AncestorType=ListBox}, Path=DataContext.CommandColorButtonSave}" />
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>
                                            </DatePickerTextBox>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                                <GridViewColumn Header="Конец">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <DatePickerTextBox Width="130"
                                                               Background="{Binding Path=operation.ColorBrush}"
                                                               Text="{Binding Path=dateTimeFinish,
                                                                              Mode=TwoWay}">
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="SelectionChanged">
                                                        <i:InvokeCommandAction Command="{Binding RelativeSource={RelativeSource AncestorType=ListBox}, Path=DataContext.CommandColorButtonSave}" />
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>
                                            </DatePickerTextBox>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                                <GridViewColumn Header="Рабочий">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal">
                                                <ComboBox Width="170"
                                                          DisplayMemberPath="FullName"
                                                          ItemsSource="{Binding RelativeSource={RelativeSource AncestorType=ListBox},
                                                                                Path=DataContext.ListOfWorkers}"
                                                          SelectedItem="{Binding Path=worker,
                                                                                 Mode=TwoWay}">
                                                    <i:Interaction.Triggers>
                                                        <i:EventTrigger EventName="SelectionChanged">
                                                            <i:InvokeCommandAction Command="{Binding RelativeSource={RelativeSource AncestorType=ListBox}, Path=DataContext.CommandColorButtonSave}" />
                                                            <i:InvokeCommandAction Command="{Binding RelativeSource={RelativeSource AncestorType=ListBox}, Path=DataContext.CommandChangeWokerInProcess}" />
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>
                                                </ComboBox>
                                            </StackPanel>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                            </GridView>
                        </ListView.View>
                    </ListView>

                    <ListBox Width="150"
                             Margin="10,290,0,10"
                             HorizontalAlignment="Left"
                             DisplayMemberPath="FullName"
                             FontSize="9"
                             ItemsSource="{Binding Path=ListOfWorkers}"
                             SelectedItem="{Binding SelectedWorker,
                                                    Mode=TwoWay}" />

                    <ListView Margin="165,290,10,10" ItemsSource="{Binding Path=ListOfLoadingWorker}">
                        <ListView.ItemContainerStyle>
                            <Style TargetType="{x:Type ListViewItem}">
                                <Setter Property="Background" Value="{Binding Path=operation.ColorBrush}" />
                            </Style>
                        </ListView.ItemContainerStyle>
                        <ListView.View>
                            <GridView>
                                <GridViewColumn Header="Операция">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal">
                                                <ComboBox Width="170"
                                                          DisplayMemberPath="nameOperation"
                                                          ItemsSource="{Binding RelativeSource={RelativeSource AncestorType=ListBox},
                                                                                Path=DataContext.ListOfOperations}"
                                                          SelectedItem="{Binding Path=operation,
                                                                                 Mode=TwoWay}">
                                                    <i:Interaction.Triggers>
                                                        <i:EventTrigger EventName="SelectionChanged">
                                                            <i:InvokeCommandAction Command="{Binding RelativeSource={RelativeSource AncestorType=ListBox}, Path=DataContext.CommandColorButtonSave}" />
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>
                                                </ComboBox>
                                            </StackPanel>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                                <GridViewColumn Header="Деталь">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal">
                                                <ComboBox Width="150"
                                                          ItemsSource="{Binding Path=ListProcess_has_detail}"
                                                          SelectedItem="{Binding Path=detail,
                                                                                 Mode=TwoWay}">
                                                    <ComboBox.ItemTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Horizontal">
                                                                <CheckBox IsChecked="{Binding FlagProcess_has_detail}" />
                                                                <TextBlock Text="{Binding Path=detail.standartdetail.nameStandartDetail}" />
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </ComboBox.ItemTemplate>
                                                    <i:Interaction.Triggers>
                                                        <i:EventTrigger EventName="SelectionChanged">
                                                            <i:InvokeCommandAction Command="{Binding RelativeSource={RelativeSource AncestorType=ListBox}, Path=DataContext.CommandColorButtonSave}" />
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>
                                                </ComboBox>
                                            </StackPanel>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                                <GridViewColumn Header="Начало">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <DatePickerTextBox Width="130"
                                                               Background="{Binding Path=operation.ColorBrush}"
                                                               Text="{Binding Path=dateTimeStart,
                                                                              Mode=TwoWay}">
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="SelectionChanged">
                                                        <i:InvokeCommandAction Command="{Binding RelativeSource={RelativeSource AncestorType=ListBox}, Path=DataContext.CommandColorButtonSave}" />
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>
                                            </DatePickerTextBox>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                                <GridViewColumn Header="Конец">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <DatePickerTextBox Width="130"
                                                               Background="{Binding Path=operation.ColorBrush}"
                                                               Text="{Binding Path=dateTimeFinish,
                                                                              Mode=TwoWay}">
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="SelectionChanged">
                                                        <i:InvokeCommandAction Command="{Binding RelativeSource={RelativeSource AncestorType=ListBox}, Path=DataContext.CommandColorButtonSave}" />
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>
                                            </DatePickerTextBox>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                                <GridViewColumn Header="Рабочий">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal">
                                                <ComboBox Width="170"
                                                          DisplayMemberPath="FullName"
                                                          ItemsSource="{Binding RelativeSource={RelativeSource AncestorType=ListBox},
                                                                                Path=DataContext.ListOfWorkers}"
                                                          SelectedItem="{Binding Path=worker,
                                                                                 Mode=TwoWay}">
                                                    <i:Interaction.Triggers>
                                                        <i:EventTrigger EventName="SelectionChanged">
                                                            <i:InvokeCommandAction Command="{Binding RelativeSource={RelativeSource AncestorType=ListBox}, Path=DataContext.CommandColorButtonSave}" />
                                                            <i:InvokeCommandAction Command="{Binding RelativeSource={RelativeSource AncestorType=ListBox}, Path=DataContext.CommandChangeWokerInProcess}" />
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>
                                                </ComboBox>
                                            </StackPanel>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                            </GridView>
                        </ListView.View>
                    </ListView>

                    <ScrollViewer Height="275"
                                  Margin="990,10,10,0"
                                  VerticalAlignment="Top"
                                  HorizontalScrollBarVisibility="Visible"
                                  VerticalScrollBarVisibility="Visible">
                        <ItemsControl Width="700"
                                      Height="275"
                                      ItemsSource="{Binding Path=ListOfRectanglesOrder}">
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <Canvas Background="#FFF6F8FB" />
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                            <ItemsControl.ItemContainerStyle>
                                <Style TargetType="ContentPresenter">
                                    <Setter Property="Canvas.Left" Value="{Binding Path=X}" />
                                    <Setter Property="Canvas.Top" Value="{Binding Path=(Y)}" />
                                </Style>
                            </ItemsControl.ItemContainerStyle>
                            <ItemsControl.ItemTemplate>
                                <DataTemplate>
                                    <Grid>
                                        <Rectangle Width="{Binding Path=Width}"
                                                   Height="24"
                                                   Fill="{Binding Path=Color}" />
                                        <TextBlock HorizontalAlignment="Center"
                                                   Foreground="Black"
                                                   Text="{Binding Path=DuringString}" />
                                    </Grid>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                        </ItemsControl>
                    </ScrollViewer>
                </Grid>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
